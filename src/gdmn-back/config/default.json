{
  "server": {
    "cluster": {
      "enabled": true,
      "workersCount": 0
    },
    "appProcess": {
      "pool": {
        "min": 0,
        "max": 3,
        "acquireTimeout": "1m",
        "idleTimeout": "1h"
      }
    },
    "http": {
      "enabled": true,
      "host": "localhost",
      "port": 4000
    },
    "stomp": {
      "heartbeat": {
        "incoming": "30s",
        "outgoing": "30s"
      }
    },
    "publicDir": "./public",
    "session": {
      "timeout": "3h",
      "maxConnections": 10
    },
    "task": {
      "timeout": "3h"
    },
    "jwt": {
      "secret": "DefaultJWTSecret",
      "token": {
        "access": {
          "timeout": "3h"
        },
        "refresh": {
          "timeout": "7d"
        }
      }
    }
  },
  "db": {
    "driver": "firebird",
    "server": {
      "host": "localhost",
      "port": 3050
    },
    "user": "SYSDBA",
    "password": "masterkey",
    "dir": "./databases",
    "pool": {
      "min": 0,
      "max": 1E3,
      "acquireTimeout": "1m",
      "idleTimeout": "1m"
    }
  }
}
