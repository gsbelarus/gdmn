{
  "name": "gdmn-front",
  "description": "gdmn web client",
  "scripts": {
    "--- gdmn ---": "",
    "build": "yarn run pkg:build && yarn run pkg:deploy",
    "clean": "shx rm -rf ./dist ./build",
    "rebuild": "yarn run clean && yarn run build",
    "start": "yarn run pkg:start",
    "watch": "tsc -w",
    "------------": "",
    "ci:compile": "yarn run clean && tsc -p ./tsconfig.json",
    "ci:prebuild": "cd ../../ && yarn run clean && cd ./src/gdmn-front && yarn run yarn:reinstall",
    "ci:build": "yarn run ci:prebuild && yarn run build",
    "ci:start": "yarn run ci:prebuild && yarn run start",
    "pkg:build": "cross-env TS_NODE_PROJECT=configs/tsconfig.webpack.json webpack --progress --config configs/webpack/webpack.config.prod.ts",
    "pkg:deploy": "shx rm -rf ../gdmn-back/public && shx cp -r ./dist ../gdmn-back/public",
    "pkg:start": "cross-env TS_NODE_PROJECT=configs/tsconfig.webpack.json webpack-dev-server --config configs/webpack/webpack.config.dev.ts",
    "prettier:cli": "prettier \"**/*.@(ts|tsx|js|jsx)\"",
    "prettier:fix": "yarn run prettier:cli -- --write",
    "prettier:lint": "yarn run prettier:cli -- -l",
    "yarn:clean": "cd ../../ && lerna exec -- shx rm -rf node_modules",
    "yarn:reinstall": "yarn run yarn:clean && yarn"
  },
  "dependencies": {
    "@stomp/stompjs": "^5.2.0",
    "@uifabric/fluent-theme": "^0",
    "es6-error": "^4",
    "gdmn-grid": "1.0.0",
    "gdmn-internals": "1.0.0",
    "gdmn-nlp": "1.0.0",
    "gdmn-nlp-agent": "1.0.0",
    "gdmn-orm": "1.0.0",
    "gdmn-recordset": "1.0.0",
    "immutable": "^3",
    "jwt-decode": "^2",
    "office-ui-fabric-react": "^6",
    "react": "^16",
    "react-content-loader": "^4",
    "react-css-modules": "^4",
    "react-dom": "^16",
    "react-redux": "^7",
    "react-router-dom": "^5",
    "recompose": "^0",
    "redux": "^4",
    "redux-persist": "^5.11.0-alpha",
    "redux-thunk": "^2",
    "rxjs": "^6",
    "typesafe-actions": "^4"
  },
  "devDependencies": {
    "@types/html-webpack-plugin": "^3",
    "@types/jest": "^24",
    "@types/jwt-decode": "^2",
    "@types/mini-css-extract-plugin": "^0",
    "@types/optimize-css-assets-webpack-plugin": "^1",
    "@types/react": "^16",
    "@types/react-content-loader": "^3",
    "@types/react-css-modules": "^4",
    "@types/react-dom": "^16",
    "@types/react-redux": "^7",
    "@types/react-router-dom": "^4",
    "@types/recompose": "^0",
    "@types/redux-logger": "^3",
    "@types/uglifyjs-webpack-plugin": "^1",
    "@types/webpack": "^4",
    "@types/webpack-bundle-analyzer": "^2",
    "@types/webpack-dev-server": "^3",
    "@types/webpack-merge": "^4",
    "awesome-typescript-loader": "^5",
    "clean-webpack-plugin": "^2",
    "compression-webpack-plugin": "^2",
    "cross-env": "^5",
    "css-loader": "^2",
    "error-overlay-webpack-plugin": "^0",
    "html-webpack-plugin": "^3",
    "jest": "^24",
    "mini-css-extract-plugin": "^0",
    "optimize-css-assets-webpack-plugin": "^5",
    "prettier": "^1",
    "react-hot-loader": "^4",
    "redux-logger": "^3",
    "shx": "^0",
    "style-loader": "^0",
    "terser-webpack-plugin": "^1",
    "ts-loader": "^5",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3",
    "webpack": "^4",
    "webpack-bundle-analyzer": "^3",
    "webpack-cli": "^3",
    "webpack-dev-server": "^3",
    "webpack-merge": "^4"
  },
  "private": true,
  "version": "1.0.0-alpha.1"
}
